<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee          http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AddDoctor</servlet-name>
        <servlet-class>Controller.AddDoctor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddPatient</servlet-name>
        <servlet-class>Controller.AddPatient</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLogin</servlet-name>
        <servlet-class>Controller.AdminLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminRegister</servlet-name>
        <servlet-class>Controller.AdminRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>lab</servlet-name>
        <servlet-class>Controller.lab</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>upr</servlet-name>
        <servlet-class>Controller.upr</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uprep</servlet-name>
        <servlet-class>uprep</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>upppp</servlet-name>
        <servlet-class>Controller.upppp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>Controller.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete</servlet-name>
        <servlet-class>Controller.delete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddDoctor</servlet-name>
        <url-pattern>/AddDoctor/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddPatient</servlet-name>
        <url-pattern>/AddPatient/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/AdminLogin/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminRegister</servlet-name>
        <url-pattern>/AdminRegister/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>lab</servlet-name>
        <url-pattern>/lab/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>upr</servlet-name>
        <url-pattern>/upr/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uprep</servlet-name>
        <url-pattern>/uprep/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>upppp</servlet-name>
        <url-pattern>/upppp/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file->index.jsp</welcome-file->
    </welcome-file-list>
</web-app>
